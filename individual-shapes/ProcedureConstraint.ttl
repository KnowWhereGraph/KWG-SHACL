#### These are SHACL shapes for the SOSA ontology -- sosa:Procedure####
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix sosa-shacl: <http://stko-kwg.geog.ucsb.ed/sosa-shacl/> .
@prefix time: <http://www.w3.org/2006/time#> .

### Shapes for class sosa:Procedure
sosa-shacl:ProcedureConstraint
	a sh:NodeShape ;
	sh:targetClass ssn:Procedure ;
	sh:property [
		sh:path ssn:hasInput ;
		sh:and (
			[
			rdfs:comment "We constrain the filler of ssn:hasInput to ssn:Input." ;
			sh:class ssn:Input
			]
		)
	] ;
	sh:property [
		sh:path ssn:hasOutput ;
		sh:and (
			[
			rdfs:comment "We constrain the filler of ssn:hasOutput to ssn:Output." ;
			sh:class ssn:Output
			]
		)
	] ;
	sh:property [
		sh:path ssn:implementedBy ;
		sh:and (
			[
			rdfs:comment "We constrain the filler of ssn:implementedBy to ssn:System." ;
			sh:class ssn:System
			]
		)
	] .