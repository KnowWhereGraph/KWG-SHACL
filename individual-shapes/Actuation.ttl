#### These are SHACL shapes for the SOSA ontology -- sosa:Actuation####
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix sosa-shacl: <http://stko-kwg.geog.ucsb.ed/sosa-shacl/> .
@prefix time: <http://www.w3.org/2006/time#> .

### Shape for class sosa:Actuation
sosa-shacl:SampleConstraint
	a sh:NodeShape ;
	sh:targetClass sosa:Actuation ;
	
	sh:property [
	 	sh:path sosa:madeByActuator ;
	 	rdfs:comment "An actuation is made by one and only one actuator that is an instance of sosa:Actuator."@en ;
		sh:class sosa:Actuator ;
		sh:minCount 1 ; 
		sh:maxCount 1 ; 
	];
	sh:property [
	 	sh:path sosa:usedProcedure ;
	 	rdfs:comment "An actuation only uses a procedure that is an instance of sosa:Procedure."@en ;
		sh:class sosa:Procedure ;
		sh:minCount 1 ; 
		sh:maxCount 1 ; 
	];
	sh:property [
	 	sh:path sosa:hasFeatureOfInterest ;
	 	rdfs:comment "An actuation has one and only one feature of interest that is an instance of sosa:FeatureOfInterest."@en ;
		sh:class sosa:FeatureOfInterest ;
		sh:minCount 1 ; 
		sh:maxCount 1 ; 
	];
	sh:property [
	 	sh:path sosa:actsOnProperty ;
	 	rdfs:comment "An actuation acts on atleast one property that is an instance of sosa:ActuatableProperty."@en ;
		sh:class sosa:ActuatableProperty ;
		sh:minCount 1 ; 
	];
	sh:property [
	 	sh:path sosa:hasResult ;
	 	rdfs:comment "An actuation acts on atleast one result that is an instance of sosa:Result."@en ;
		sh:class sosa:Result ;
		sh:minCount 1 ; 
	];
	sh:property [
	 	sh:path sosa:resultTime ;
	 	rdfs:comment "An actuation has exactly one and only one result time."@en ;
		sh:minCount 1 ; 
		sh:minCount 1 ; 
	].
	